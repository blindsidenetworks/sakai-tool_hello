<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

	<!-- We need this file to satisfy Spring, but we don't need anything in it since we use Wicket's dependency injection -->
    <!-- Entity provider implementation 
    <bean id="org.sakaiproject.evaluation.logic.entity.HelloEntityProvider"
        class="org.sakaiproject.evaluation.logic.impl.entity.HelloEntityProviderImpl">
    </bean>
    -->
    
    <!-- Web entity provider -->
    <bean parent="org.sakaiproject.entitybroker.entityprovider.AbstractEntityProvider"
        class="org.sakaiproject.hello.tool.entity.HelloWorldEntityProvider">
        <property name="helloManager" ref="org.sakaiproject.hello.logic.HelloManager"/>
        <property name="userDirectoryService" ref="org.sakaiproject.user.api.UserDirectoryService" />
    </bean>

</beans>
